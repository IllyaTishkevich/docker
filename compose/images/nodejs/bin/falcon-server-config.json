{
  "appName": "falcon-server-demo",
  "debug": false,
  "port": 4000,
  "logLevel": "info",
  "verboseEvents": false,
  "endpoints": {
    "magento": {
      "package": "@deity/falcon-magento2-api/src/endpoints",
      "config": {
        "protocol": "https",
        "port": "8443",
        "host": "nginx"
      }
    }
  },
  "apis": {
    "magento2": {
      "package": "@deity/falcon-magento2-api",
      "config": {
        "host": "nginx",
        "port": "8443",
        "protocol": "https",
        "auth": {
          "type": "admin-token",
          "username": "admin",
          "password": "password1"
        },
        "payments": {
          "adyen_cc": {
            "key": "10001|"
          }
        },

        "currency": {
          "symbol": "USD"
        },

        "demo": true,
        "enableSwitcher": true,
        "enableAutoDetection": false,
        "geoMapping": {
          "*": "default",
          "NL": "nl_store",
          "PL": "pl_store",
          "FR": "fr_store",
          "DE": "de_store"
        },
        "languageMapping": {
          "en": ["*"],
          "nl": ["nl_store"],
          "pl": ["pl_store"],
          "de": ["de_store"],
          "fr": ["fr_store"]
        },
        "defaultStoreCode": "default",
        "codeMapping": {
          "default": "default",
          "nl_store": "nl_store",
          "pl_store": "pl_store",
          "fr_store": "fr_store",
          "de_store": "de_store"
        }
      }
    }
  },
  "extensions": {
    "shop": {
      "package": "@deity/falcon-shop-extension",
      "config": {
        "api": "magento2"
      }
    }
  },
  "session": {
    "keys": ["secret key"],
    "options": {
      "key": "session-key",
      "maxAge": 86400000,
      "overwrite": true,
      "httpOnly": true,
      "signed": true,
      "rolling": false,
      "renew": false
    }
  },
  "cache": {
    "url": "/cache",
    "resolvers": {
      "enabled": true,
      "invalidation": true,
      "default": {
        "ttl": 10
      },
      "query.menu": {
        "ttl": 15
      }
    }
  }
}
